<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.redvendedores.controlador.ContactosViewControler"
            prefHeight="400.0" prefWidth="600.0">

    <ListView fx:id="listViewContactos" layoutX="50.0" layoutY="50.0" prefHeight="300.0" prefWidth="200.0" />

    <Button fx:id="btnAgregarContacto" layoutX="300.0" layoutY="50.0" onAction="#agregarContacto" text="Agregar Contacto" />

</AnchorPane>
